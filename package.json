{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "front": "cd front && yarn start",
    "api": "cd server && nodemon index.js",
    "dev": "concurrently \"npm run api\" \"npm run front\"",
    "start": "node server/index.js",
    "heroku-postbuild": "npm run-script build",
    "migrate": "cd server && sqitch deploy",
    "migrate:reset": "cd server && sqitch revert && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^26.0.23",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.17",
    "form-data": "^4.0.0",
    "joi": "^17.4.0",
    "mocha": "^9.0.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "supertest": "^6.1.3"
  },
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "babel-jest": "^27.0.2",
    "cross-env": "^7.0.3",
    "jest": "^27.0.4"
  }
}
